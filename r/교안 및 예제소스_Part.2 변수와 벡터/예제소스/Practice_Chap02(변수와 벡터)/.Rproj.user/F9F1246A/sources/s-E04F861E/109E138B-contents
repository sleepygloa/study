2+3

(3+6)*10

# 2의 제곱
2^2  

# %나머지 연산자
10%%3  # 몫:3, 나머지:1

# 함수 사용
log(10)+5  #로그함수 
sqrt(100)  #제곱근
max(10,5,7,-8,1000)  #최대값을 구함
min(10,5,7,-8,1000)  #최소값을 구함

# 변수(Variable) - 얼마든지 변할 수 있는 값 
a <- 10  # a변수에다가 10을 저장함.
# <- 의 단축키 Alt + -
b <- 20
c <- a + b
c
print(c)  # print()함수를이용하여 값을 출력함.

a <- "a"
a + b # 문자+숫자는 연산이 안됨

b <- "b"
a + b  # 문자+문자는 연산이 안됨

# 논리형(TRUE, FALSE)
c <- F
c 

# NULL은 아무값을 지니지 않는다.
d <- NULL
d
# NaN값을 출력
sqrt(-10)

# N/A --> 결측치, 누락된 값 




