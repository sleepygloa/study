<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.seonhoblog.samplelogin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MenuActivity"></activity>
        <activity android:name=".Layout.LayoutActivity"></activity>
        <activity android:name=".Layout.RelativeViewActivity"></activity>
        <activity android:name=".Layout.FrameViewActivity"></activity>
        <activity android:name=".Layout.GridViewActivity"></activity>
        <activity android:name=".Layout.LinearViewActivity"></activity>
        <activity android:name=".Layout.TotalViewActivity"></activity>
        <activity android:name=".Layout.ContraintViewActivity"></activity>
        <activity android:name=".Layout.ScrollViewActivity"></activity>

    </application>

</manifest>